<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hamsik2046.password"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.MyTheme" >
        <activity
            android:name="com.hamsik2046.password.activity.WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hamsik2046.password.activity.SetPasswordActivity"/>
        <activity
            android:name="com.hamsik2046.password.activity.LoginActivity"/>
        <activity
            android:name="com.hamsik2046.password.activity.PasswordListActivity"/>
        <activity android:name="com.hamsik2046.password.activity.AddAccountActivity"/>
    </application>

</manifest>
